  },
  "investigation_steps": {
    "triage_steps": "Initial steps include verifying the incident scope, collecting logs, and isolating the affected system to prevent further propagation.",
    "evidence_collection": [
      {
        "description": "Collect raw logs from firewalls, intrusion detection systems, and domain controllers.",
        "command": "sudo tail -f /var/log/firewall.log && sudo grep -i ' suspicious' /var/log/auth.log"
      },
      {
        "description": "Acquire network traffic captures for analysis.",
        "command": "tcpdump -i eth0 -w /tmp/network_traffic.pcap"
      }
    ],
    "technical_analysis": "Analyze logs for indicators of compromise, such as unusual login attempts, IP addresses, or malicious scripts.",
    "tools_and_commands": [
      {
        "tool": "Volatility",
        "usage": "Memory analysis for detecting malware remnants.",
        "example": "sudo volatility.exe -y -f /tmp/memorydump"
      },
      {
        "tool": "Wireshark",
        "usage": "Network traffic analysis for identifying command and control traffic.",
        "example": "sudo wireshark -i /tmp/network_traffic.pcap"
      }
    ],
    "indicators_of_compromise": [
      "IP addresses: 192.168.1.100, 192.168.2.101",
      "Hashes: MD5: abc123, SHA1: def456",
      "File paths: /tmp/malicious_script.sh"
    ]
  },
  "containment_procedures": {
    "immediate_actions": "Disconnect the compromised system from the network and preserve evidence without altering systems.",
    "system_isolation": "Use iptables to block all traffic from the system: sudo iptables -A INPUT -s 192.168.1.100 -j DROP",
    "malicious_activity_blocking": "Block known malicious IPs and domains: sudo ufw block 192.168.1.100",
    "evidence_preservation": "Copy sensitive data to a secure location: sudo dd if=/tmp/sensitive_data of=/secure/location/saved_data"
  },
  "eradication_steps": {
    "threat_removal": "Remove all malicious files and scripts using tools like ClamAV: sudo clamscan /tmp/malicious_files",
    "vulnerability_fix": "Patch the legacy system to address the exploited vulnerability: sudo apt-get update && sudo apt-get upgrade",
    "lateral_movement_check": "Run chkrootkit to check for lateral movement: sudo chkrootkit -t /tmp"
  },
  "recovery_procedures": {
    "system_restoration": "Restore the system from a known good backup: sudo rsync -avz /backups/ /restored_system",
    "integrity_validation": "Verify system integrity using checksums: sha256sum /etc/passwd",
    "return_to_operations": "Re-enable services only after confirming the system is secure: sudo systemctl enable --now sshd"
  },
